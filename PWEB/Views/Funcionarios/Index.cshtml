@model IEnumerable<PWEB_AulasP_2223.ViewModels.GestoresViewModel>

@{
    ViewData["Title"] = "Gestores";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}


    <h1>Gestores</h1>
    <table class="table">
        <thead>
            <tr>
                <th>
                    Primeiro Nome
                </th>
                <th>
                    Último Nome
                </th>
                <th>
                    Email
                </th>
                <th>
                    Empresa
                </th>
                <th>
                    Ações
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.PrimeiroNome)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.UltimoNome)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Email)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.NomeEmpresa)
                    </td>
                    <td></td>
                </tr>
            }
        </tbody>
    </table>
